#!/bin/bash -e

aws ecr get-login-password --region ap-southeast-2 | docker login --username AWS --password-stdin 349430847202.dkr.ecr.ap-southeast-2.amazonaws.com

werf build --repo 349430847202.dkr.ecr.ap-southeast-2.amazonaws.com/redguava/shipit

